   "source": [
    "#Do cross validation\n",
    "ind_cutoff = int(0.7*len(X))\n",
    "X_train, Y_train = X[0:ind_cutoff], Y[0:ind_cutoff]\n",
    "X_test, Y_test = X[ind_cutoff:], Y[ind_cutoff:]"
   ]
